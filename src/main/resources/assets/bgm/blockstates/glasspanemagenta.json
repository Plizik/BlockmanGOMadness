{
  "multipart": [
    {
      "apply": {
        "model": "bgm:block/glasspanemagenta_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "bgm:block/glasspanemagenta_noside",
        "y": 270
      }
    }
  ]
}